{
  "name": "aircall-slack-agent",
  "version": "1.0.0",
  "description": "On-demand agent to monitor Aircall activity and report to Slack",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "node test.js",
    "report:afternoon": "node index.js --report afternoon",
    "report:night": "node index.js --report night",
    "report:custom": "node index.js --report custom",
    "heroku-postbuild": "echo 'Build completed successfully!'"
  },
  "keywords": [
    "aircall",
    "slack",
    "monitoring",
    "on-demand"
  ],
  "author": "Joel Schrock",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "cron-parser": "^5.3.0",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-prometheus-middleware": "^1.2.0",
    "express-rate-limit": "^8.0.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "prom-client": "^13.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sirschrockalot/Slack-KPI-Service.git"
  },
  "homepage": "https://github.com/sirschrockalot/Slack-KPI-Service#readme",
  "deployment": {
    "environment": "production",
    "platform": "heroku",
    "last_deployment_test": "2025-08-26"
  }
}
